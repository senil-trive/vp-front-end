{
  "name": "vp-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "docs": "npx styleguidist server",
    "docs:build": "npx styleguidist build --prefix docs/",
    "start": "next start",
    "server": "pm2 start npm --name 'vp-website' -- start -- -- -p 8005",
    "server:dev": "pm2 start npm --name 'vp-website-dev' -- start -- -- -p 8001",
    "server:staging": "pm2 start npm --name 'vp-website-staging' -- start -- -p 8003",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build"
  },
  "dependencies": {
    "@sentry/nextjs": "^7.26.0",
    "@types/node": "18.11.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.6",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/styled-components": "^5.1.26",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "daisyui": "^2.47.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.0",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "2.8.1",
    "react-styleguidist": "^13.0.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.4"
  }
}
