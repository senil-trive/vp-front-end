{
  "name": "vp-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "server": "pm2 start npm --name 'vp-website' -- start -- -- -p 8005",
    "server:dev": "pm2 start npm --name 'vp-website-dev' -- start -- -- -p 8001",
    "server:staging": "pm2 start npm --name 'vp-website-staging' -- start -- -p 8003",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sentry/nextjs": "^7.26.0",
    "@types/node": "18.11.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.6.0",
    "prettier": "2.8.1",
    "husky": "^8.0.0"
  }
}
